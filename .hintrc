{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "user-select",
          "scrollbar-width"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "video[playsinline]",
          "input[capture]",
          "li[is]",
          "meta[name=theme-color]",
          "video[controlslist]"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "no-inline-styles": "off",
    "manifest-file-extension": "off"
  }
}